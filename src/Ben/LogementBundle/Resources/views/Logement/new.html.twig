{% extends "BenLogementBundle::layout.html.twig" %}
{% block title %}
Tableau de bord | {{ parent() }}
{% endblock %}
{% block body %}

<h1>Logement creation</h1>

<form class="form-horizontal" action="{{ path('logement_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}

    <div class="form-group">
      <div class="col-md-offset-4 col-md-4">
        <button type="submit" class="btn btn-primary"><span class="glyphicon glyphicon-plus"></span> Ajouter</button>
      </div>
    </div>
</form>
{% endblock body %}
