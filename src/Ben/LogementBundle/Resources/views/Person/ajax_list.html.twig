{% for entity in entities %}
    <tr>
        <td class="hidden-print"><input type="checkbox" name="entities[]" value="{{ entity.id }}" ></td>
        <td>{{ entity.ndossier }}</td>
        <td><em> {{ entity.fullName }}</em></td>
        <td>{{ entity.cin }}</td>
        <td>{{ entity.cne }}</td>
        <td>{{ entity.status }}</td>
        <td>{{ entity.type }}</td>
        <td class="hidden-print">
        <div class="text-centers">
            <a class="btn btn-primary js-show btn-xs" href="{{ path('etudiant_show', { 'id': entity.id }) }}"><span class="glyphicon glyphicon-search"></span></a>
            <a class="btn btn-info btn-xs" href="{{ path('etudiant_edit', { 'id': entity.id, 'type': entity.type }) }}"><span class="glyphicon glyphicon-edit"></span></a>
            {% if entity.status == person.resident %}
            <a class="btn btn-default btn-xs" href="{{ path('reservation_show', { 'id': entity.reservation.id }) }}"><span class="glyphicon glyphicon-file"></span></a>{% endif %}
        </div>
        </td>
    </tr>
{% endfor %}
                
{% include "BenLogementBundle:Default:pagination.html.twig" %}