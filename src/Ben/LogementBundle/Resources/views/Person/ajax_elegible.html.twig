{% for entity in entities %}
    <tr>
        <td class="hide-print"><input type="checkbox" name="entities[]" value="{{ entity.id }}"></td>
        <td>{{ entity.ndossier }}</td>
        <td>{{ entity.fullName }}</td>
        <td>{{ entity.email }}</td>
        <td>{{ entity.cin }}</td>
        <td>{{ entity.cne }}</td>
        <td>{{ entity.status }}  {{ entity.hasReservation }}</td>
        <td>{{ entity.type }}</td>
        <td class="hide-print">
        <div class="text-center">
            <a class="btn" href="{{ path('reservation_new', { 'id': entity.id }) }}"><span class="glyphicon glyphicon-check"></span> Paiement</a>
        </div>
        </td>
    </tr>
{% endfor %}
                
{% include "BenLogementBundle:Default:pagination.html.twig" %}