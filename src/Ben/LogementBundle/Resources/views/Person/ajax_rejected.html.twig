{% for entity in entities %}
    <tr>
        <td class="hidden-print"><input type="checkbox" name="entities[]" value="{{ entity.id }}"></td>
        <td>{{ entity.ndossier }}</td>
        <td>{{ entity.fullName }}</td>
        <td>{{ entity.cin }}</td>
        <td>{{ entity.cne }}</td>
        {% if entity.status == 'archive' %}
        <td>{% if entity.created %}{{ entity.created|date('d/m/Y') }}{% endif %}</td>
        {% endif %}
        <td>{{ entity.remarque }}</td>
        <td class="hidden-print">
        <div class="text-center">
            <a class="btn btn-primary js-show btn-xs" href="{{ path('etudiant_show', { 'id': entity.id }) }}"><span class="glyphicon glyphicon-search"></span></a>
            <a class="btn btn-info btn-xs" href="{{ path('etudiant_edit', { 'id': entity.id, 'type': entity.type }) }}"><span class="glyphicon glyphicon-edit"></span></a>
        </div>
        </td>
    </tr>
{% else %}
    <tr class="mypagination">            
        <td colspan="10">
            <div id="msgbox" class="alert alert-danger fade in" role="alert"> 
                <button type="button" class="close" data-dismiss="alert">×</button>
                <strong>info !</strong> aucun résultat ne correspond à vos critères
            </div>
        </td>
    </tr>
{% endfor %}
                
{% include "BenLogementBundle:Default:pagination.html.twig" %}