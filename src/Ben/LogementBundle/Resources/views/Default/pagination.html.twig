{% if entities|length > nombreParPage %}
<tr class="mypagination hide-print">
    <td colspan="10">
      <div class="text-center">
          <ul class="pagination">
            {% if page != 1 %}
            <li class="previous"><a href="#" class="js-page" data-page="{{ page-1 }}"><span class="glyphicon glyphicon-chevron-left"></span></a></li>
            {% endif %}
            {% for p in range(1, nombrePage) %}
            <li{% if p == page %} class="active"{% endif %}>
              <a href="#" class="js-page" data-page="{{ p }}" >{{ p}}</a>
            </li>
            {% endfor %}
            {% if page != nombrePage %}
              <li class="next"><a href="#" class="js-page" data-page="{{ page+1 }}"><span class="glyphicon glyphicon-chevron-right"></span></a></li>
            {% endif %}
          </ul>
      </div>
    </td>
</tr>
{% endif %}